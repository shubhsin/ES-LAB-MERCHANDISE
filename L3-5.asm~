; Bubble sort 10 unsigned bytes

MOV 30h,#05h
MOV 31h,#02h
MOV 32h,#0Ah
MOV 33h,#06h
MOV 34h,#07h
MOv 35h,#03h
MOV 36h,#08h 
MOV 37h,#50h
MOV 38h,#40h
MOV 39h,#22h

LOOP1:	MOV DPTR,#30H
	MOV A,R0
	MOV R1,A
LOOP2:	MOVC A,@A+DPTR
	MOV R2,A
	INC DPTR
	MOVC A,@A+DPTR
	SUBB A,R2
	JNC CONT2
SWAP1:	MOVC A,@A+DPTR
	XCH A,R2
	MOV 50H,A
	MOVC @A+DPTR,A
	INC DPTR
CONT2:	DJNZ R1,LOOP2
CONT1:	DJNZ R0,LOOP1
here:	SJMP here
