MOV 40H,#02H
MOV 41H,#04H
MOV 42H,#08H
MOV 43H,#0CH
MOV 33,40H
MOV 34,41H

MOV R2,#3
MOV R0,#40H
MOV R1,#41H

	MOV 33H,@R0
	MOV 34H,@R1
LOOP:	ACALL GCD_TWO
	INC R1
	MOV 34H,@R1
	DJNZ R2,LOOP
	
here:SJMP here

GCD_TWO:
UP:MOV A,33H
CJNE A,34h,down
SJMP exit
down:	JC bga
	MOV A,33h
	CLR C
	SUBB A,34h
	MOV 33h,A
	SJMP UP
bga:	MOV A,34h
	CLR C
	SUBB A,33h
	MOV 34h,A
	SJMP UP
exit:	MOV 32H,33H
	RET