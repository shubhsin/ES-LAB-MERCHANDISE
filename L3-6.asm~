MOV SP,#40H
MOV R2,#98h
CLR P1.0
ACALL BCD_HEX
MOV R1,#59H
CLR A
UP:	ADD A,R1
	DA A
	JNC down
	XCH A,R3
	ADD A,#1
	DA A
	XCH A,R3
DOWN:	DJNZ R2,UP
	MOV 30H,A
	MOV 31H,R3
here:	SJMP here

BCD_HEX:	PUSH A
	PUSH B
	MOV A,R2
	ANL A,#0F0H
	SWAP A
	MOV B,#0AH
	MUL AB
	ANL 02,#0Fh
	ADD A,R2
	MOV R2,A
	POP B
	POP A
	RET